  "license": "ISC",
  "dependencies": {
    "markdown-it": "^13.0.0",
    "markdown-it-container": "^3.0.0",
    "node-sass": "^7.0.1",
    "node-sass-tilde-importer": "^1.0.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  }
}
